// Solving the quadratic equation

var fun := func(a, b) => print "govno"


var findRoots := func(a, b, c) is

    print empty

    a := (2+2)*2;

    // If a is 0, then equation is not
    // quadratic, but linear

    if (a = 0) then
       print "Invalid";
       return;
    end

    var d := b*b - 4*a*c;
    var sqrtValue := sqrt(abs(d));

    if (d > 0) then
       print "Roots are real and different \n";
       print (-b + sqrtValue) / (2 * a), "\n", (-b - sqrtValue) / (2 * a);
    else // d < 0
       print "Roots are complex \n";
       print -b / ( 2 * a ), " + i", sqrtValue;
       print "\n";
	   print -b / ( 2 * a ), " - i", sqrtValue;
	end
end

print "Enter coefficients a, b, c\n";
var a := readInt,
	  b := readInt,
		c := readInt;

findRoots(a, b, c);